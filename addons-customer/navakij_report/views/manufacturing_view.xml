<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_manufacturing_form_inherit" model="ir.ui.view">
        <field name="name">mrp.production.form</field>
        <field name="model">mrp.production</field>
        <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='button_unbuild']" position="after">
                <button string="Print Manufacturing (TH)" name="print_manufacturing_TH" type="object" />
                <button string="Print Manufacturing (ENG)" name="print_manufacturing_ENG" type="object" />            
            </xpath>
        </field>
    </record>

</odoo>