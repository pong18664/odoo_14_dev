<odoo>

    <!-- dummy invoice report th -->
    <record id="invoice_report_dummy_th_pdf" model="ir.actions.report">
        <field name="name">Print Invoice Dummy</field>
        <field name="model">account.move</field>
        <field name="report_name">nawakij_dummy_report.template_invoice_report_dummy</field>
        <field name="report_file">nawakij_dummy_report.template_invoice_report_dummy</field>
        <field name="binding_model_id" ref="account.model_account_move" />
        <field name="binding_type">report</field>
        <field name="report_type">qweb-pdf</field>
        <field name="print_report_name">('report invoice (TH) - %s' % (object.name))</field>
        <field name="paperformat_id" ref="nawakij_dummy_report.dummy_invoice_report_paperformat" />
    </record>

